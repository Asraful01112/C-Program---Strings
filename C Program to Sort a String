#include <stdio.h>
#include <string.h>

int main() {
    char str[100];
    int i, j, min;
    char temp;

    printf("Enter a string: ");
    scanf("%99s", str);

    int len = strlen(str);

    for (i = 0; i < len - 1; i++) {
        min = i;
        for (j = i + 1; j < len; j++) {
            if (str[j] < str[min]) {
                min = j;
            }
        }
        temp = str[i];
        str[i] = str[min];
        str[min] = temp;
    }

    printf("Sorted string: %s\n", str);

    return 0;
}
