#include <stdio.h>
#include <ctype.h>
#include <string.h>

int isPangram(char str[]) {
    int letters[26] = {0}; 
    int i;

    for (i = 0; str[i] != '\0'; i++) {
        if (isalpha(str[i])) {
            char ch = tolower(str[i]);
            letters[ch - 'a'] = 1;  
        }
    }

    for (i = 0; i < 26; i++) {
        if (letters[i] == 0)
            return 0;  
    }
    return 1; 
}

int main() {
    char str[200];

    printf("Enter a string: ");
    fgets(str, sizeof(str), stdin);

    if (isPangram(str))
        printf("The string is a Pangram.\n");
    else
        printf("The string is NOT a Pangram.\n");

    return 0;
}
